{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": [
      "es6"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": ".",
    "paths": {
      "": [
        "app/"
      ],
      "tests": [
        "tests/*"
      ],
      "@components/*": [
        "app/components/*"
      ],
      "@utils/*": [
        "./app/utils/*"
      ],
      "@elements/*": [
        "./app/elements/*"
      ],
      "@nav/*": [
        "./app/nav/*"
      ],
      "@assets/*": [
        "./app/assets/*"
      ],
      "@screens/*": [
        "./app/screens/*"
      ],
      "@actions/*": [
        "./app/actions/*"
      ],
      "@api/*": [
        "./app/api/*"
      ],
      "@config/*": [
        "./app/config/*"
      ],
      "@navigation/*": [
        "./app/navigation/*"
      ],
      "@reducers/*": [
        "./app/reducers/*"
      ],
      "@sagas/*": [
        "./app/sagas/*"
      ],
      "@store/*": [
        "./app/store/*"
      ],
      "@i18n": [
        "./app/i18n/*"
      ],
      "@styles/*": [
        "./app/styles/*"
      ],
      "@constants/*": [
        "./app/constants/*"
      ],
    }
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ],
}